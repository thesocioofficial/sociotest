{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://sociotest-production.up.railway.app",
    "NEXT_PUBLIC_APP_URL": "https://sociotest.vercel.app"
  },
  "functions": {
    "app/auth/callback/route.ts": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/discover",
      "destination": "/Discover"
    }
  ]
}
